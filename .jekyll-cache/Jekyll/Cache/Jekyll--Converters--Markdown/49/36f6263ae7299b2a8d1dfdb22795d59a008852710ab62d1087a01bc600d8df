        <span class="k">if</span><span class="p">(</span><span class="n">abs</span><span class="p">(</span><span class="n">y2</span><span class="o">-</span><span class="n">yyy</span><span class="o">-</span><span class="n">y</span><span class="p">)</span><span class="o">+</span><span class="n">abs</span><span class="p">(</span><span class="n">x2</span><span class="o">-</span><span class="n">x1</span><span class="o">-</span><span class="n">x</span><span class="p">)</span><span class="o">&lt;=</span><span class="n">i</span><span class="o">+</span><span class="mi">1</span><span class="p">){</span>
