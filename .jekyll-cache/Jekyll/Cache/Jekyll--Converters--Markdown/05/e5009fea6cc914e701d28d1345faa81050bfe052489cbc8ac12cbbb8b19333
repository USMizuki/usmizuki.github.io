            <span class="n">cout</span><span class="o">&lt;&lt;</span><span class="p">(</span><span class="n">ll</span><span class="p">)</span><span class="n">a</span><span class="o">*</span><span class="n">n</span><span class="o">&lt;&lt;</span><span class="n">endl</span><span class="p">;;</span>
        <span class="p">}</span>
        <span class="k">else</span> <span class="p">{</span>
            <span class="k">if</span><span class="p">(</span><span class="n">n</span><span class="o">%</span><span class="mi">2</span><span class="o">==</span><span class="mi">0</span><span class="p">){</span>
