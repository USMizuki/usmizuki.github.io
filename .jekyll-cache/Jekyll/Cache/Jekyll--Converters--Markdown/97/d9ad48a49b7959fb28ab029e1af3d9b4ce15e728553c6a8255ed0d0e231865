        <span class="n">cout</span><span class="o">&lt;&lt;</span><span class="n">ans</span><span class="p">[</span><span class="n">i</span><span class="p">];</span>
        <span class="n">cout</span><span class="o">&lt;&lt;</span><span class="p">((</span><span class="n">i</span><span class="o">==</span><span class="n">n</span><span class="p">)</span><span class="o">?</span><span class="sc">'\n'</span><span class="o">:</span><span class="sc">' '</span><span class="p">);</span>
    <span class="p">}</span>
<span class="p">}</span>
</code></pre></div></div>

